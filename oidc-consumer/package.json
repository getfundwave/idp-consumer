{
  "name": "@fundwave/oidc-consumer",
  "version": "2.0.0",
  "description": "OIDC consumer middlewares and utilities",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "dev": "tsc --watch",
    "predocs": "npm run build",
    "docs": "npx jsdoc-to-markdown dist/index.js > DOCUMENTATION.md",
    "prepublish": "npm run docs",
    "test": "jest"
  },
  "keywords": [
    "oidc",
    "consumer",
    "oidc-consumer",
    "oauth"
  ],
  "author": "fundwave-authors",
  "license": "MIT",
  "dependencies": {
    "@types/express-session": "^1.17.5",
    "@types/simple-oauth2": "^5.0.4",
    "express-session": "^1.17.3",
    "minimatch": "^9.0.3",
    "simple-oauth2": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@types/express": "^4.17.14"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^8.0.1",
    "sinon": "^17.0.2",
    "sinon-express-mock": "^2.2.1",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.4"
  }
}
